<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PAS_APP</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PAS_APP.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/DataTable/DataTable.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/DataTable/dataTables.bootstrap4.min.css" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
    <!-- Table -->
     @* <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" media="all">  *@
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.css" media="all">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.css" media="all">
     <link rel="stylesheet" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap.min.css" media="all">
    <!-- table end -->


</head>
<body>
    <header>
        <!-- Sidebar -->
        <div class="sidebar" id="sidebar">
            <div class="sidebar-header">
                <div class="sidebar-logo">
                    <i class="fa-solid fa-p fa-xl"></i>
                </div>
                <div class="sidebar-title">Profile Analysis System</div>
            </div>
            <nav class="sidebar-nav">
                <a href="dashboard-modern.html" class="sidebar-item active">
                    <div class="sidebar-icon">
                        <i class="fas fa-home"></i>
                    </div>
                    <span class="sidebar-text">Dashboard</span>
                </a>
                <a href="messages-modern.html" class="sidebar-item">
                    <div class="sidebar-icon">
                        <i class="fas fa-comments"></i>
                    </div>
                    <span class="sidebar-text">Messages</span>
                </a>
                <a href="ai-chat-modern.html" class="sidebar-item">
                    <div class="sidebar-icon">
                        <i class="fas fa-robot"></i>
                    </div>
                    <span class="sidebar-text">Service</span>
                </a>
                <a href="profile.html" class="sidebar-item">
                    <div class="sidebar-icon">
                        <i class="fa-solid fa-file-lines"></i>
                    </div>
                    <span class="sidebar-text">Form</span>
                </a>
                <a href="profile.html" class="sidebar-item">
                    <div class="sidebar-icon">
                        <i class="fa-solid fa-filter"></i>
                    </div>
                    <span class="sidebar-text">Start Filter</span>
                </a>
                <a href="profile.html" class="sidebar-item">
                    <div class="sidebar-icon">
                        <i class="fas fa-user"></i>
                    </div>
                    <span class="sidebar-text">Profile</span>
                </a>
                <button class="sidebar-item" onclick="logout()">
                    <div class="sidebar-icon">
                        <i class="fas fa-sign-out-alt"></i>
                    </div>
                    <span class="sidebar-text">Logout</span>
                </button>
            </nav>
        </div>
    </header>
    <div >
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            PAS; Profile Analysis System - <a asp-area="" asp-controller="" asp-action="">Start Filtering</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/DataTable/dataTables.bootstrap4.min.js"></script>
    <script src="~/lib/DataTable/jquery.dataTables.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>

    <!--Datatable JS-->
    <script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>

    <script>
        $(document).ready(function() {
            $('#example').DataTable();
        } );
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
